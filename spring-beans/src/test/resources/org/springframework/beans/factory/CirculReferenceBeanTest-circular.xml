<?xml version="1.0" encoding="ISO-8859-1"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
                       https://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	   default-autowire="byType">
	
	<bean id="beanA" class="org.springframework.beans.factory.BeanA">
		<property name="name" value="beanA"/>
		<property name="beanB" ref="beanB"/>
	</bean>

	<bean id="beanB" class="org.springframework.beans.factory.BeanB">
		<property name="name" value="beanB"/>
		<property name="beanA" ref="beanA"/>
	</bean>

	<bean id="beanC" class="org.springframework.beans.factory.BeanC" />
	
<!--	<bean id="myInstantiationAwareBeanPostProcessor" class="org.springframework.beans.factory.MyInstantiationAwareBeanPostProcessor" />-->
</beans>
